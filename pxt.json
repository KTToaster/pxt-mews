{
    "name": "mews",
    "version": "1.0.0",
    "description": "A PXT Extension to allow for Text to speech on the microbit - using word-stringing",
    "dependencies": {
        "core": "*",
        "microphone": "*"
    },
    "files": [
        "README.md",
        "textToSpeech.ts",
        "mews.ts",
        "source/mappings.ts",
        "source/audio.h",
        "source/audio.cpp",
        "source/utils/string_utils.ts",
        "source/utils/decompressor.h",
        "source/utils/decompressor.cpp",
        "source/textToSpeech.cpp"
    ],
    "testFiles": [
        "main.blocks",
        "main.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "8.1.6",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "blocksprj",
    "extension": {
        "namespace": "tannoy",
        "label": "Precompile",
        "url": "http://localhost:5173",
        "localUrl": "http://localhost:5173"
    }
}
