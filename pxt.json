{
    "name": "mews",
    "version": "0.0.1",
    "description": "A PXT Extension to allow for Text to speech on the microbit - using word-stringing",
    "dependencies": {
        "core": "*",
        "microphone": "*"
    },
    "files": [
        "README.md",
        "tannoy.ts",
        "source/mappings.ts",
        "source/tannoy.cpp",
        "source/mappings.h",
        "source/audio/audio.h",
        "source/audio/numbers_minimum.h",
        "source/audio/stations_minimum.h",
        "source/audio/numbers_extreme.h",
        "source/audio/numbers_high.h",
        "source/audio/numbers_medium.h",
        "source/audio/numbers_low.h",
        "source/audio/stations_extreme.h",
        "source/audio/stations_high.h",
        "source/audio/stations_medium.h",
        "source/audio/stations_low.h",
        "source/utils/decompressor.h",
        "source/utils/decompressor.cpp",
        "source/tannoy.cpp"
    ],
    "testFiles": [
        "main.blocks",
        "main.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "8.1.6",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "blocksprj",
    "yotta": {
        "config": {
            "AUDIO_QUALITY": 1
        }
    },
    "extension": {
        "namespace": "tannoy",
        "label": "Initialise",
        "url": "http://localhost:5173",
        "localUrl": "http://localhost:5173"
    }
}
